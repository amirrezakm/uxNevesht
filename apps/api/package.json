{
  "name": "@ux-nevesht/api",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --exec \"ts-node --transpile-only\" src/index.ts",
    "dev-simple": "nodemon --exec \"ts-node --transpile-only\" src/index-simple.ts",
    "dev-enhanced": "nodemon --exec \"ts-node --transpile-only\" src/index-enhanced.ts",
    "dev-ultra": "nodemon --exec \"ts-node --transpile-only\" src/index-ultra-optimized.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start-ultra": "node dist/index-ultra-optimized.js",
    "type-check": "tsc --noEmit",
    "fix-vectorization": "node src/scripts/fix-vectorization.js",
    "test-fixes": "ts-node src/test-fixes.ts",
    "setup-db": "ts-node src/scripts/database-setup.ts",
    "test-system": "ts-node src/scripts/test-system.ts",
    "startup-check": "ts-node src/scripts/startup-check.ts"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "@supabase/supabase-js": "^2.38.4",
    "@ux-nevesht/ai": "workspace:*",
    "@ux-nevesht/database": "workspace:*",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}